      <div class="request_call_menu_body">
        <form action="" class="apply_form">
          <p class="light_text">Оставьте заявку</p>
          <h4 class="apply_form_title">Свяжитесь с нами в 2 клика</h4>
          <p class="subtitle">
            Менеджер перезвонит в течение 15 минут и подберет наиболее удобное
            время.
          </p>
          <div class="inputs">
            <input
              class="form_input"
              type="text"
              name="name"
              required=""
              placeholder="Введите имя"
            />
            <input
              class="form_input"
              type="text"
              name="city"
              required=""
              placeholder="Введите город"
            />
            <input
              class="form_input"
              type="tel"
              required=""
              placeholder="Введите телефон "
              id="phone2"
            />
            <textarea
              placeholder="Сообщение"
              class="form_input"
              name=""
              id=""
            ></textarea>
            <button type="submit">Оставить заявку</button>
          </div>
        </form>
      </div>
    </div>
