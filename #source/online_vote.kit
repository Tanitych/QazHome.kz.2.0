    <div class="request_call_menu">
      <div class="close_btn">
        <svg
          xmlns="http://www.w3.org/2000/svg"
          width="40"
          height="40"
          viewBox="0 0 40 40"
          fill="none"
        >
          <rect width="40" height="40" fill="#EAEAEA" />
          <path
            fill-rule="evenodd"
            clip-rule="evenodd"
            d="M19.116 20L14.558 24.558L15.442 25.442L20 20.884L24.558 25.442L25.442 24.558L20.884 20L25.442 15.442L24.558 14.558L20 19.116L15.442 14.558L14.558 15.442L19.116 20Z"
